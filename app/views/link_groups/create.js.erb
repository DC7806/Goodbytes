<% if @ajax_create_group[:ok] %>

  $(`#create-group`).modal('hide')
  $('.modal-backdrop').hide()
  $('#create-group .group_name').val('');

  $(`.link_group`).replaceWith("<%= j render('shared/partials/create_link_group', link_groups: @link_groups, saved_link: @saved_link) %>")

  <% else %>
    <%= @ajax_create_group[:message] %>
  <% end %>